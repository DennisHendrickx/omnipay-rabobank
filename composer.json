{
    "name": "omnipay/rabobank",
    "type": "library",
    "description": "Rabobank Omnikassa driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "ideal",
        "omnikassa",
        "rabobank"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-rabobank",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-rabobank/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Rabobank\\" : "src/" }
    },
    "require": {
        "php": "^5.6|^7",
        "omnipay/common": "^3.0.2"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3",
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^1.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
